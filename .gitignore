# Ignore WordPress core files
public_html/wp-admin/
public_html/wp-includes/
public_html/license.txt
public_html/readme.html
public_html/wp-salt.php

# Ignore debug logs
public_html/wp-content/debug.log

# Ignore all plugins and themes except your custom ones
public_html/wp-content/plugins/*
!public_html/wp-content/plugins/your-custom-plugin/

public_html/wp-content/themes/*
!public_html/wp-content/themes/jampack/

# Ignore uploads (optional)
public_html/wp-content/uploads/

# Ignore backups
public_html/wp-content/ai1wm-backups/

# Ignore sensitive files
public_html/wp-config.php
public_html/wp-config-sample.php
.env
*.sql
*.sqlite
*.db

# OS/IDE files
.DS_Store
Thumbs.db
.vscode/
.idea/

# Node/npm build artifacts (if used)
node_modules/
dist/
build/

# Avoid ignoring the database creation script
!/jampack_database/createDB.sql

# Ignore Docker logs directory
docker_apache/logs/
